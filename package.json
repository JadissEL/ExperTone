{
  "name": "expert-intelligence-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:setup": "node scripts/setup-db.js",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "ml:start": "cd ml-service && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "infra:up": "cd infra && docker compose up -d",
    "infra:down": "cd infra && docker compose down",
    "n8n:push": "npx tsx scripts/push-workflow.ts",
    "expiry:fake-age": "npx tsx scripts/fake-expert-age.ts",
    "db:seed": "npx tsx scripts/seed.ts",
    "db:seed-embeddings": "npx tsx scripts/seed-with-embeddings.ts",
    "db:backfill-embeddings": "npx tsx scripts/backfill-experts-without-embeddings.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "audit": "npm audit --audit-level=high",
    "typecheck": "tsc --noEmit",
    "ml:lint": "cd ml-service && python -m ruff check .",
    "ml:format": "cd ml-service && python -m black .",
    "ml:typecheck": "cd ml-service && python -m mypy . --ignore-missing-imports",
    "ml:test": "cd ml-service && python -m pytest -v",
    "ml:bandit": "cd ml-service && python -m bandit -c pyproject.toml -r . -x ./tests"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.0.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/postcss": "^4.2.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/three": "^0.183.0",
    "autoprefixer": "^10.4.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.2",
    "lucide-react": "^0.575.0",
    "next": "14.2.25",
    "openai": "^4.0.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph-2d": "^1.29.1",
    "react-force-graph-3d": "^1.29.1",
    "react-hook-form": "^7.71.1",
    "swr": "^2.4.0",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.0",
    "three": "^0.183.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.25",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.1",
    "prisma": "^6.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  }
}
